@model IEnumerable<Project.Models.Course>
@{
    ViewBag.Title = "Course";
    Layout = "~/Views/Shared/CRUD/Index.cshtml";
}

<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LectureDuration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LabDuration)
        </th>
        <th></th>
    </tr>
</thead>

<tbody id="tbody">
    @foreach (var item in Model)
    {
        @Html.Partial("Row", item)
    }
</tbody>