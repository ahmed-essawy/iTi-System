<div class="row">
    <div class="table-responsive">
        <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <td>Name</td>
                    <td>Email</td>
                </tr>
            </thead>
            <tbody>
                @foreach (Student item in ViewBag.StList)
                {
                    <tr id="@item.Id">
                        <td>@item.Name                </td>
                        <td>@item.Email                </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
