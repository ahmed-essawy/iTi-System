@model IEnumerable<Project.Models.Permission>
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Instructor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reason)
        </th>
    </tr>
</thead>
<tbody id="tbody">
    @foreach (var item in Model)
    {
        <tr id="@item.Id">
            <td>@item.Student.Name</td>
            <td>@item.StartDate</td>
            <td>@item.EndDate</td>
            <td>@item.Instructor.Name</td>
            <td>
                <input type="button" value="Reason" class="btn btn-info btn-xs" onclick="$('#DataModal').modal('show').find('#ModalBody').text('@item.Reason')" />
            </td>
        </tr>
    }
</tbody>