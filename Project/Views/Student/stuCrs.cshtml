@using Project.Models
@model IEnumerable<Project.Models.CourseGrade>

@{
    ViewBag.Title = "stuCrs";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/CPanel.cshtml";
}
<h2>stuCrs</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LabGrade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExamGrade)
        </th>
        <th></th>
    </tr>
    @foreach (CourseGrade item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CourseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LabGrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExamGrade)
            </td>
        </tr>
    }
</table>