@model Project.Models.Student
<tr id="@Model.Id">
    <td>
        @Html.DisplayFor(model => model.Name)
    </td>
    <td>
        @Html.DisplayFor(model => model.Department.Name)
    </td>
    <td>
        @Html.DisplayFor(model => model.Email)
    </td>
    <td>
        @Html.DisplayFor(model => model.UserName)
    </td>
    <td>
        @Html.DisplayFor(model => model.PhoneNumber)
    </td>
    <td>
        @Html.DisplayFor(model => model.Birthdate)
    </td>
    <td>
        @Html.DisplayFor(model => model.IsMarried)
    </td>
    <td>
        @Html.DisplayFor(model => model.Degrees)
    </td>
    <td>
        @Ajax.ActionLink("Edit", "Edit", new { id = Model.Id }, new AjaxOptions() { HttpMethod = "Get", InsertionMode = InsertionMode.ReplaceWith, UpdateTargetId = "ModalBody" }, new { @class = "btn btn-warning", @onclick = "$('#DataModal').modal('show')" }) |
        <input type="button" value="Delete" class="btn btn-danger" onclick="Delete('@Model.Id')" />
    </td>
</tr>